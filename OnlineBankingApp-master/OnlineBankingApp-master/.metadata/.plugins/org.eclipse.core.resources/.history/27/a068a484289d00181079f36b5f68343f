	public MMBankCustomer(int customerId, String customerName, long contactNumber, String emailId,
			LocalDate dateOfBirth, Address permanentAddress, String nationality, String gender) {
		super(customerId, customerName, contactNumber, emailId, dateOfBirth, permanentAddress, nationality, gender);
		// TODO Auto-generated constructor stub
	}

	

}
